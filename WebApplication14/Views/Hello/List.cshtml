@model IEnumerable<WebApplication14.Models.Book>
@{
    ViewData["Title"] = "List";
    //Layout = "_MyLayout";
}
<h1>List</h1>
@*<partial name="_Time" />*@
@*@await Component.InvokeAsync("List", new { price = 3000 })*@

<table class="table">
    <thead>
        <tr>
            <th>書名</th>
            <th>価格</th>
            <th>著者</th>
            <th>配布サンプル</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Title</td>
                <td>@item.Price</td>
                <td>@item.Publisher</td>
                <td>@item.Sample</td>
            </tr>
        }
    </tbody>
</table>